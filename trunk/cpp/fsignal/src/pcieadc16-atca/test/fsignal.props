#Nameservice location
#ORBInitRef=NameService=corbaloc:iiop:1.2@localhost:1050/NameService
ORBInitRef=NameService=corbaloc:iiop:1.2@baco.ipfn.ist.utl.pt:1050/NameService

#max size allowed to transport in bytes
#ORBgiopMaxMsgSize=1677721600
ORBgiopMaxMsgSize=16777216

#The id of the central server
CServerID=CentralServerRegister

#The location of the file that will exist when you want to terminate the program
FileExitName=exit

#Check for the above file every N seconds
FileExitSleepCheck=10

#The location of the dir where to store serialized data
PersistDir=persist

#The location of the node XML
#NodeXMLFileLoc=/home/ipfnist/code/scad/trunk/cpp/fsignal/src/pcieadc-atca/test/xml/TestNode.xml
NodeXMLFileLoc=xml/TestNode.xml

#The location of the hardware XML files, separated by commas
#HardwareXMLFilesLoc=/home/ipfnist/code/scad/trunk/cpp/fsignal/src/pcieadc-atca/test/xml/Hardware1.xml
HardwareXMLFilesLoc=xml/Hardware1.xml,xml/Hardware2.xml
#HardwareXMLFilesLoc=xml/Hardware3.xml
#HardwareXMLFilesLoc=xml/Hardware2.xml
#HardwareXMLFilesLoc=xml/Hardware7.xml,xml/Hardware8.xml, xml/Hardware9.xml
#HardwareXMLFilesLoc=xml/Hardware7.xml,xml/Hardware8.xml

#The locale in which your xmls are writen
XML_Locale=portuguese

#The location of the extra lib
ExtraLibLoc=/home/andre/test.jar

#The location of the dev files. It must be on the same order of the HardwareXMLFilesLoc
#DevFileLocations=/dev/adcDaq6,/dev/adcDaq7,/dev/adcDaq8
#DevFileLocations=/dev/adcDaq8,/dev/adcDaq6
#DevFileLocations=/dev/adcDaq14,/dev/adcDaq10
DevFileLocations=/dev/pcieAdcDaq.8,/dev/pcieAdcDaq.6

#The maximum amount of bytes per cluster
#BufferSendSize=8388608
BufferSendSize=12582912

