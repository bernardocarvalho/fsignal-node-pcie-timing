<hardware   uniqueID="BOARD_1">
    <name xml:lang="en">Board 1</name>
    <description xml:lang="en">Board 1</description>
    <field  uniqueID="TRIGGER_SECS"
            editable="true"
            emptyAllowed="false"
            hardwareField="no">
        <name xml:lang="en">Trigger Delay(sec)</name>
        <description xml:lang="en">The channel trigger delay in seconds</description>
        <type>
            <continuous primitive="int">
                <min>0</min>
                <max>3600</max>
                <step>1</step>  
                <defaultValue>1</defaultValue>
            </continuous>        
        </type>
    </field>
    <field  uniqueID="TRIGGER_NSECS"
            editable="true"
            emptyAllowed="false"
            hardwareField="no">
        <name xml:lang="en">Trigger Delay(nsec)</name>
        <description xml:lang="en">The channel trigger delay in nano seconds</description>
        <type>
            <continuous primitive="int">
                <min>0</min>
                <max>1000000000</max>
                <step>8</step>  
                <defaultValue>0</defaultValue>
            </continuous>        
        </type>
    </field>
    <field  uniqueID="PULSE_NSECS"
            editable="true"
            emptyAllowed="false"
            hardwareField="no">
        <name xml:lang="en">Pulse Width(nsec)</name>
        <description xml:lang="en">The channel trigger pulse Width in nano seconds</description>
        <type>
            <continuous primitive="int">
                <min>8</min>
                <max>1000000000</max>
                <step>8</step>  
                <defaultValue>800000000</defaultValue>
            </continuous>        
        </type>
    </field>
