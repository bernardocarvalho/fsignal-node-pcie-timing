<!DOCTYPE hardware SYSTEM "Hardware.dtd">

<hardware   uniqueID="BOARD_3">
    <name xml:lang="en">Board 3</name>
    <description xml:lang="en">Board 3</description>
    <field  uniqueID="FREQUENCY"
            editable="false"
            emptyAllowed="false"
            hardwareField="yes">
        <name xml:lang="en">Frequency</name>
        <description xml:lang="en">The board acquisition frequency</description>
        <type>
            <continuous primitive="int">
                <min>2000000</min>
                <max>2000000</max>
                <step>1</step>  
                <defaultValue>2000000</defaultValue>
            </continuous>        
        </type>
    </field>
    <field  uniqueID="TRIGGER_TYPE"
            editable="true"
            emptyAllowed="false"            
            hardwareField="yes">
        <name xml:lang="en">Trigger type</name>
        <description xml:lang="en">The type of trigger to use</description>
        <type>
            <list>
                <element>Software</element>
                <element>Hardware</element>
                <defaultValue>Hardware</defaultValue>
            </list>
        </type>
    </field>
    <field  uniqueID="ACQ_TIME_MILLIS"
            editable="true"
            emptyAllowed="false"
            hardwareField="no">
        <name xml:lang="en">Acquisition time (ms)</name>
        <description xml:lang="en">Defines the amount of data which will be transferred from the board for this channel</description>
        <type>
            <continuous primitive="double">
                <min>0.5</min>
                <max>2000</max>
                <step>0.5</step>  
                <defaultValue>1</defaultValue>
            </continuous>        
        </type>
    </field>
    <field  uniqueID="MEM_SIZE"
            editable="false"
            emptyAllowed="false"
            hardwareField="yes">
        <name xml:lang="en">On board memory size</name>
        <description xml:lang="en">The amount of memory available on the board</description>
        <type>
            <continuous primitive="int">
                <min>536870911</min>
                <max>536870911</max>
                <step>1</step>  
                <defaultValue>536870911</defaultValue>
            </continuous>        
        </type>
    </field>
    <field  uniqueID="DMA_BYTE_SIZE"
            editable="false"
            emptyAllowed="false"
            hardwareField="yes">
        <name xml:lang="en">DMA byte bize</name>
        <description xml:lang="en">The size of each DMA in bytes</description>
        <type>
            <continuous primitive="int">
                <min>4096</min>
                <max>4096</max>
                <step>1</step>  
                <defaultValue>4096</defaultValue>
            </continuous>        
        </type>
    </field>
