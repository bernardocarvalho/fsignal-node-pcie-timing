#The location of the node XML
#NodeXMLFileLoc=/home/bernardo/Projects/cpp/fsignal/src/tr512/xml/Tr512Node.xml
NodeXMLFileLoc=xml/Tr512Node.xml

#The location of the hardware XML files, separated by commas
#HardwareXMLFilesLoc=tr512/xml/Hardware0.xml,/home/bernardo/Projects/cpp/fsignal/src/tr512/xml/Hardware1.xml,/home/bernardo/Projects/cpp/fsignal/src/tr512/xml/Hardware2.xml
HardwareXMLFilesLoc=xml/Hardware0.xml
#, tr512/xml/Hardware1.xml  


#The locale in which your xmls are writen
XML_Locale=portuguese

#The location of the extra lib
ExtraLibLoc=/home/andre/test.jar

